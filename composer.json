{
    "name": "sandbox-dev/laravel-html",
    "description": "HTML and Form Builders for the Laravel Framework",
	"type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandbox Dev",
			"email": "info@sandbox.co.th"
        }
    ],
    "require": {
		"php": "^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/routing": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/session": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/view": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.5|^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Sandbox\\Html\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        },
        "laravel": {
            "providers": [
                "Sandbox\\Html\\HtmlServiceProvider"
            ],
            "aliases": {
                "Form": "Sandbox\\Html\\FormFacade",
                "Html": "Sandbox\\Html\\HtmlFacade"
            }
        }
    },
    "prefer-stable": true
}
